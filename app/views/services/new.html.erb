<div class="container my-5">
  <h1>New service</h1>
  <%= simple_form_for(@service) do |f| %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :cost %>
    <%= f.association :category, label_method: :category%>
    <%= f.input :user_id, collection: User.all, label_method: :first_name, value_method: :id %>

    <%= f.submit %>
  <% end %>
</div>

